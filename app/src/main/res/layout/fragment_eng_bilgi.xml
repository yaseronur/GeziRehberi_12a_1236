<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="FragmentTutucu.Fragment_Eng_Bilgi">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/editText_YerAdiGir"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Enter It's Name"
            android:inputType="text" />

        <EditText
            android:id="@+id/editText_UlkesiniGir"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Enter It's Country"
            android:inputType="text" />

        <EditText
            android:id="@+id/editText_SehriGir"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Enter It's City"
            android:inputType="text" />

        <EditText
            android:id="@+id/editText_TarihcesiniGir"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Enter It's History"
            android:inputType="text" />

        <EditText
            android:id="@+id/editText_HakkÄ±ndaGir"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:ems="10"
            android:hint="Enter It's About"
            android:inputType="text" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>